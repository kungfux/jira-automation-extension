{
  "manifest_version": 3,
  "name": "IssueExpert for JIRA",
  "version": "1.0",
  "description": "Add extra functionality to JIRA for operating with issues.",
  "icons": {
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.atlassian.net/jira/*"],
      "js": ["bundle.js"]
    }
  ],
  "host_permissions": ["*://*.atlassian.net/jira/*"]
}
